@{
    var keys = ViewBag.Keys as string[];
}
<h2>QR Preview</h2>
<div>
    @foreach(var k in keys)
    {
        <div style="display:inline-block;margin:16px;text-align:center">
            <img src="/qr/generate?key=@k" style="width:220px;height:220px;border:1px solid #ddd;padding:8px" />
            <div>@k</div>
        </div>
    }
</div>